<tr>
  <%@prod = Product.where(id: prod_id)%>
  <td>
    <%= image_tag @prod[0].image.tiny %>
  </td>
  <td>
    <h4><%= @prod[0].name %></h4>
    <br>
    <p><%= @prod[0].description %></p>
  </td>
  <td>
    <%= humanized_money_with_symbol @prod[0].price %>
  </td>
  <td>
  <%= quantity %>
  <td>
    <%= humanized_money_with_symbol @prod[0].price * quantity %>
  </td>
</tr>
